pipeline{
	agent any 
	stages {
		stage('Build') {
			steps {
				echo "I am in build stage of uat1"
			}
		}
		stage('Test') {
			steps {
				echo "I am in test stageof uat1 "
			}
		}
		stage('Deploy') {
			steps {
				echo "I am in deploy stage of uat1"
			}
		}
				stage('Archive') {
			steps {
				echo "I am in deploy Archive stage of uat1"
		      }
		}
	 }

}
